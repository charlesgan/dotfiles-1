URxvt*background: #1A1A1A
URxvt*foreground: #ffffff
URxvt*termName: xterm-color
URxvt.scrollBar: false

URxvt*font: xft: Inconsolata:size=16

Xft.font: xft: Inconsolata:size=16

Xft.antialias: true
Xft.dpi: 96
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.autohint: 1
Xft.rgba: rgb


URxvt.perl-ext-common:  default,matcher,clipboard
URxvt.urlLauncher:      /usr/bin/chromium
URxvt.matcher.button:   1


! black
*color0: #192033
*color8: #666666
! red
*color1: #A62A3E
*color9: #F04758
! green
*color2: #38912B
*color10: #93C724
! yellow
*color3: #B27D12
*color11: #DDB62B
! blue
*color4: #355C9A
*color12: #45A3E6
! magenta
*color5: #7C4F9F
*color13: #C953EF
! cyan
*color6: #258F8F
*color14: #60C6C8
! white
*color7: #77858C
*color15: #C0C0C0
